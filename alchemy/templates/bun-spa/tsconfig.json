{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "alchemy.run.ts",
    "bun-env.d.ts"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@cloudflare/workers-types"
    ]
  }
}
